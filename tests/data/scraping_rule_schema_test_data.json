// Data Presented as tuple, with first element being data, second elem is null if the data should load through the scraping rule schema, or a string exception name if an exception should be raised. Exception names must be one from the enum basketbot.BasketBotErrors
// Correct scraping-rule JSON
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "BR", "classes": ["fandangaling", "jimminy"]}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "1": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}}}, null]
// Correct scraping-rule JSON with empty class chain
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {}}, null]
// Entirely incorrect class-chain
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": "ohdear"}, "InvalidClassChain"]
// Class chain keys are not all numeric
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "BR", "classes": ["fandangaling", "jimminy"]}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "one": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}}}, "InvalidClassChain"]
// Class chain keys do not increment
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "BR", "classes": ["fandangaling", "jimminy"]}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "2": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}, "3": {"tree_node": {"dom_type": "DIV", "classes": ["more", "classes"]}}}}, "InvalidClassChain"]
// Class chain keys do not start at zero
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"1": {"tree_node": {"dom_type": "BR", "classes": ["fandangaling", "jimminy"]}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "2": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}, "3": {"tree_node": {"dom_type": "DIV", "classes": ["more", "classes"]}}}}, "InvalidClassChain"]
// Class chain tree_node has class names that are not a list
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "BR", "classes": "this_should_be_in_a_list"}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "1": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}}}, "InvalidClassChain"]
// Class chain tree_node has class names that are not strings
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "BR", "classes": [55, "jimminy"]}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "1": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}}}, "InvalidClassChain"]
// Parent elem is of html tag type not in DB
[{"parent_elem": "OHDEAR", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "BR", "classes": ["fandangaling", "jimminy"]}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "1": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}}}, "InvalidDOMElem"]
// A tree_node from class-chain is of html tag type not in DB
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "OHDEAR", "classes": ["fandangaling", "jimminy"]}, "siblings": [{"dom_type": "DIV", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "1": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}}}, "InvalidDOMElem"]
// A sibling from class-chain is of html tag type not in DB
[{"parent_elem": "A", "parent_id": "swashbuckling", "retail_site": "http://example.com", "items": [], "class_chain": {"0": {"tree_node": {"dom_type": "BR", "classes": ["fandangaling", "jimminy"]}, "siblings": [{"dom_type": "OHDEAR", "classes": ["lalala", "funtimes"]}, {"dom_type": "#text", "classes": ["classy", "stuff"]}]}, "1": {"tree_node": {"dom_type": "DIV", "classes": ["goodness", "willickers"]}}}}, "InvalidDOMElem"]
